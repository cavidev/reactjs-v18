{
    "files.eol": "\n",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.ignoreLegacyWarning": true,
    "workbench.editorAssociations": {
        "*.svg": "default"
    },
    "terminal.integrated.env.windows": {},
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ffffff",
        "terminalCursor.foreground": "#ffffff"
    },
    "terminal.integrated.cursorStyle": "underline",
    "editor.cursorBlinking": "expand",
    "editor.fontFamily": "'Victor Mono', 'JetBrains Mono', 'Anonymous Pro', monospace",
    "editor.tabSize": 4,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontSize": 16,
    "editor.fontWeight": "200",
    "editor.codeLensFontFamily": "'Anonymous Pro', 'JetBrains Mono'",
    "editor.fontVariations": true,
    "editor.experimental.asyncTokenization": true,
    "editor.fontLigatures": "'ss02'",
    "editor.guides.bracketPairs": false,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.stickyScroll.enabled": true,
    "oneDarkPro.bold": false,
    "errorLens.fontStyleItalic": true,
    "errorLens.fontFamily": "'Anonymous Pro'",
    "files.autoSave": "onFocusChange",
    "editor.inlayHints.fontFamily": "'Anonymous Pro', 'JetBrains Mono'",
    "editor.inlineSuggest.fontFamily": "'Anonymous Pro', 'JetBrains Mono', default",
    "debug.console.fontFamily": "'Anonymous Pro', 'JetBrains Mono', default",
    "scm.inputFontFamily": "'Anonymous Pro', 'JetBrains Mono', default",
    "gitlens.blame.fontFamily": "'Anonymous Pro', 'JetBrains Mono', default",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "always"
    },
    "terminal.integrated.fontFamily": "'Anonymous Pro'",
    "workbench.colorTheme": "One Dark Pro Darker",
    "js-auto-backticks.enableRevert": true,
    "prettier.printWidth": 100,
    "editor.formatOnSave": true,
    "prettier.configPath": ".prettierrc",
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSaveMode": "file",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4,
        "editor.formatOnSave": true
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4,
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnType": false,
    "html.format.indentInnerHtml": true,
    "autoimport.filesToScan": "**/*.{ts,tsx,js,jsx}"
}
